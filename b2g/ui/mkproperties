#!/bin/sh
set -e

. ../../properties.sh
. ../../resources.sh

printResourcesHeader
printResource integer app_version_code "${versionCode}"
printResource string app_version_name "${versionNumber}"
printResource string app_source_revision "${sourceRevision}"
printResource string app_build_time "${buildDate}@${buildTime} ${buildZone}"
printResourcesFooter

exit 0
