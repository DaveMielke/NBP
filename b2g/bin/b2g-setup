#!/bin/bash
set -e

cd "$(dirname "${0}")"
export PATH=".:${PATH}"

b2g-set no-device-lock on
b2g-set screen-timeout 1800000
b2g-set foreign-sources on

b2g-set accessibility-services on
b2g-set b2g-input-service on
b2g-set b2g-screen-monitor on
b2g-set brltty-service on
b2g-set talkback-service on
b2g-set spiel-service off

exit 0
