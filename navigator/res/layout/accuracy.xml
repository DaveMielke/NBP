<?xml version="1.0" encoding="utf-8"?>

<merge xmlns:android="http://schemas.android.com/apk/res/android">
  <TextView
    android:text="@string/section_accuracy"
    android:textAppearance="?android:attr/textAppearanceLarge"
    android:paddingRight="@dimen/spacer_width"
   />

  <TableRow android:id="@+id/row_satellites">
    <TextView android:text="@string/label_satellites" />
    <include layout="@layout/spacer" />
    <TextView android:id="@+id/accuracy_satellites" />
    <include layout="@layout/spacer" />
  </TableRow>

  <TableRow android:id="@+id/row_horizontal">
    <TextView android:text="@string/label_horizontal" />
    <include layout="@layout/spacer" />
    <TextView android:id="@+id/accuracy_horizontal" />
    <include layout="@layout/spacer" />
  </TableRow>
</merge>
